#!/bin/bash
set -a
# Load .env variables
source /home/ubuntu/powerBAI/.env
set +a

# Check that env vars are loaded
echo "ALLOWED_HOSTS=$ALLOWED_HOSTS"

# Launch Gunicorn using your venv-installed binary
/home/ubuntu/powerBAI/powerenv/bin/gunicorn --workers 3 --bind unix:/home/ubuntu/powerBAI/powerBAI.sock project.wsgi:application


